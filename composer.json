{
    "name": "nachofassini/laravel-database-structure",
    "description": "Package that generates a php file with the tables and fields of the database after running all the migrations",
    "keywords": [
        "database",
        "laravel",
        "structure",
        "schema",
        "schema-file"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ignacio Fassini",
            "email": "nachofassini@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "illuminate/contracts": "~5.3|~5.4",
        "illuminate/support": "~5.3|~5.4",
        "thedevsaddam/laravel-schema": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "NachoFassini\\LaravelDatabaseStructure\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NachoFassini\\LaravelDatabaseStructure\\ServiceProvider"
            ]
        }
    }
}
